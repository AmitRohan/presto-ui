{
	  "name": "Presto Panel",
      "description": "Presto Plugin for auto generation of UI"
	  "author": "Sachin Sharma",
	  "identifier" : "PLUGIN_IDENTIFIER",
	  "compatibleVersion": 47,
	  "build": "",
	  "version": "0.1.0",
    "appcast": "https://api.sketchpacks.com/v1/plugins/PLUGIN_IDENTIFIER/appcast",
    "commands": [
        {
            "script" : "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name" : "Events",
            "identifier" : "events",
            "handlers": {
                "actions": {
                    "Startup" : "onStartup",
                    "OpenDocument":"onOpenDocument",
                    "CloseDocument":"onCloseDocument",
                    "DocumentSaved": "onSaveDocument",
                    "SelectionChanged.finish" : "onSelectionChanged"
                }
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Create Background",
            "shortcut": "cmd alt b",
            "identifier": "CreateBackground",
            "handlers" : {
                "run": "createBackground"
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Export Images",
            "shortcut": "cmd alt e",
            "identifier": "ExportImages",
            "handlers" : {
                "run": "exportImage"
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Generate Page",
            "shortcut": "cmd alt p",
            "identifier": "AutogenPage",
            "handlers" : {
                "run": "autoGenPage"
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Generate Artboard",
            "shortcut": "cmd alt a",
            "identifier": "AutogenArtboard",
            "handlers" : {
                "run": "autoGenArtboard"
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Generate Selection",
            "shortcut": "cmd alt s",
            "identifier": "AutogenSelection",
            "handlers" : {
                "run": "autoGenSelection"
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Test Artboard",
            "shortcut": "cmd alt l",
            "identifier": "LogArtboard",
            "handlers" : {
                "run": "displayLogs"
            }
        },
        {
            "script": "PrestoPanelUI.framework/PrestoPanelUI.js",
            "name": "Show Props",
            "shortcut": "cmd alt g",
            "identifier": "Props",
            "handlers" : {
                "run": "displayProps"
            }
        }
		],
    "menu": {
        "isRoot" : false,
        "shortcut" : "",
        "title": "Presto Panel",
        "items": [
            "LogArtboard",
            "CreateBackground",
            "AutogenSelection",
            "AutogenArtboard",
            "AutogenPage",
            "ExportImages",
            "Props"
        ]
    }
}
